{
  "kind": "Pod",
  "apiVersion": "v1",
  "metadata": {
    "name": "lets-explore-app",
    "labels": {
      "app": "webapp"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "weather-service",
        "image": "gcr.io/explorer-152713/weather-service:v8",
        "ports": [
          {
            "containerPort": 8081
          }
        ]
      },
      {
        "name": "events-service",
        "image": "gcr.io/explorer-152713/events-service:v8",
        "ports": [
          {
            "containerPort": 8082
          }
        ]
      },
      {
        "name": "places-service",
        "image": "gcr.io/explorer-152713/places-service:v8",
        "ports": [
          {
            "containerPort": 8083
          }
        ]
      }
    ]
  }
}
